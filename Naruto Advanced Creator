#date(year: number, month: number, day: number)
#enero: 1 (31), marzo 3 (31), mayo: 5 (31), julio: 7 (31), agosto: 8 (31), octubre: 10 (31), diciembre: 12 (31)
#abril: 4 (30), junio: 6 (30), septiembre: 9 (30), noviembre: 11 (30)
#febrero: 2 (28)
#►
import:
	org.bukkit.Bukkit

on chat:
	if {Creando PJ::%player%} is set:
		cancel event
	else:
		loop all players:
			if {Creando PJ::%loop-player%} is not set:
				add loop-player to chat recipients

on join:
	if {Nombre Ninja::%player%} is not set:
		teleport player to location(56.5, 4, -350.5, "otro_mundo" parsed as world, 269, 0)
		send title "&4&l¡Crea tu Personaje!" with subtitle "&cAún no has creado tu personaje, habla con Hagoromo" to player
	if {Creando PJ::%player%} is set:
		send title "&a&2Te desconectaste" with subtitle "&aHas aparecido en el lugar que quedaste en el Creador de Personaje" to player

on NPC right click:
	if event-number is 11:
		if {Nombre Ninja::%player%} is not set:
			hide player from all players
			set {Creando PJ::%player%} to 1
			teleport player to location(-246.5, 5, -198.5, "otro_mundo" parsed as world, 180, 0)
			send title "&eIdentidad" with subtitle "&bEscoge tu nombre en Rol, escribiendolo en chat" to player
			message "&5Sistema &8► &dEscoge tu nombre en Rol, piensa bien que nombre tendrás ya que después no podrás cambiarlo. Cuando ya hayas escogido tu nombre escribe &5confirmar &den el chat." to player
		else:
			send action bar "&aHas sido teletransportado al Spawn" to player
			teleport player to spawn of the world "Naruto_Revolution_Final"
	else:
		if {Creando PJ::%player%} is 2:
			if event-number is 10: # Hombre
				set {Sexo::%player%} to "&bMasculino"
				set {Creando PJ::%player%} to 3
				send title "&b&lMasculino" with subtitle "&bAhora perteneces al género masculino" to player
				wait 2 seconds
				teleport player to location(-246.5, 5, -198.5, "otro_mundo" parsed as world, 180, 0)
				message "%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%"
				message "&5Sistema &8► &dDinos la fecha de Nacimiento que tendrá tu Personaje, esto para gestionar de forma automática tu edad en Rol. El formato a seguir es &5<dia> <mes> <tu edad>&d.%nl%&5Ejemplo: &d4 2 19, lo que equivale a 4 de Febrero de 2001"
				send title "&eFecha de Nacimiento" with subtitle "&bDinos la fecha de nacimiento de tu Personaje" to player
			else if event-number is 9: # Mujer
				set {Sexo::%player%} to "&dFemenino"
				set {Creando PJ::%player%} to 3
				send title "&d&lFemenino" with subtitle "&dAhora perteneces al género femenino" to player
				wait 2 seconds
				teleport player to location(-246.5, 5, -198.5, "otro_mundo" parsed as world, 180, 0)
				message "%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%"
				message "&5Sistema &8► &dDinos la fecha de Nacimiento que tendrá tu Personaje, esto para gestionar de forma automática tu edad en Rol. El formato a seguir es &5<dia> <mes> <tu edad>&d.%nl%&5Ejemplo: &d4 2 19, lo que equivale a 4 de Febrero de 2001"
				send title "&eFecha de Nacimiento" with subtitle "&bDinos la fecha de nacimiento de tu Personaje" to player
			else if event-number is 12: # No Binario
				set {Sexo::%player%} to "&8No Binario"
				set {Creando PJ::%player%} to 3
				send title "&8&lNo Binario" with subtitle "&8Tu sexo es No Binario" to player
				wait 2 seconds
				teleport player to location(-246.5, 5, -198.5, "otro_mundo" parsed as world, 180, 0)
				message "%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%"
				message "&5Sistema &8► &dDinos la fecha de Nacimiento que tendrá tu Personaje, esto para gestionar de forma automática tu edad en Rol. El formato a seguir es &5<dia> <mes> <tu edad>&d.%nl%&5Ejemplo: &d4 2 19 lo que equivale a 4 de Febrero de 2001"
				send title "&eFecha de Nacimiento" with subtitle "&bDinos la fecha de nacimiento de tu Personaje" to player

#! Nombre

on chat:
	if {Creando PJ::%player%} is 1:
		cancel event
		if message is "pene", "vagina", "culo", "pito", "pija", "chota", "weon", "maricon", "homosexual", "homo" or "gay":
			send title "&4&l¡Cuidado!" with subtitle "&cCrea un nombre que no sea ofensivo." to player
		else:
			if message is not "confirmar":
				set {_a::*} to message split at " "
				if size of {_a::*} is more than 1:
					send title "&4&l¡Muy Largo!" with subtitle "&cCrea un nombre más corto" to player
				else:
					set player's display name to message
					set {Nombre Ninja::%player%} to message
					send title message with subtitle "&bSi quieres este nombre escribe &econfirmar" to player
			else:
				set {Creando PJ::%player%} to 2
				teleport player to location(-55.5, 7, -128.5, "otro_mundo" parsed as world, 88, 0)
				message "%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%%nl%"
				message "&5Sistema &8► &dEscoge tu sexo tocando a uno de los NPCs. El sexo no podrás cambiarlo en un futuro, así que escoge bien." to player
				send title "&5Sexo" with subtitle "&dPor favor escoge tu sexo tocando a los NPC" to player
	else if {Creando PJ::%player%} is 3:
		cancel event
		set {_fechas::*} to message split at " "
		if {_fechas::1} parsed as integer is a integer:
			set {_dia} to {_fechas::1} parsed as number
		else:
			message "&5Sistema &8► &4¡Error! &dPor favor menciona un número válido"
			stop
		if {_fechas::2} parsed as integer is a integer:
			if {_mes} is 1, 3, 5, 7, 8, 10 or 12:
				if {_dia} is less or equal to 31:
					set {_mes} to {_fechas::2} parsed as number
				else:
					message "&5Sistema &8► &4¡Error! &dPor favor menciona un día válido... Enero, Marzo, Mayo, Julio, Agosto, Octubre y Diciembre solo tienen 31 días."
					stop
			else if {_mes} is 4, 6, 9 or 11:
				if {_dia} is less or equal to 30:
					set {_mes} to {_fechas::2} parsed as number
				else:
					message "&5Sistema &8► &4¡Error! &dPor favor menciona un día válido... Abril, Junio, Septiembre y Noviembre solo tienen 30 días."
					stop
			else if {_mes} is 2:
				if {_dia} is less or equal to 28:
					set {_mes} to {_fechas::2} parsed as number
				else:
					set {_año} to {Fecha Actual Servidor} formatted as "yyyy"
					set {_diferencia} to difference between {_fechas::3} and {_año}
					set {_año} to {_año} - {_diferencia}
					if mod({_año}, 4) is 0:
						set {_mes} to {_fechas::2}
					else:
						message "&5Sistema &8► &4¡Error! &dPor favor menciona un día válido, Febrero solo tiene 28 días, 29 si es año biciesto."
						stop
			set {_mes} to {_fechas::2} parsed as integer
		else:
			message "&5Sistema &8► &4¡Error! &dPor favor escoge una edad válida."
			stop
		if {_fechas::3} parsed as integer is between 8 and 14:
			set {_edad} to {_fechas::3} parsed as integer
		else:
			message "&5Sistema &8► &4¡Error! &dPor favor escoge una edad que ronde entre los 8 y 14 años."
			stop
		set {_año} to {Fecha Actual Servidor} formatted as "yyyy"
		set {_año} to {_año} parsed as integer
		set {_año} to {_año} - {_edad}
		set {Fecha Nacimiento::%player%} to date({_año}, {_mes}, {_dia}, 0, 0, 0, 0)
		set {_año} to {Fecha Actual Servidor} formatted as "yyyy" parsed as integer
		set {Fecha Cumpleaños::%player%} to date({_año}, {_mes}, {_dia}, 0, 0, 0, 0)

		teleport player to location(-179.5, 7, -114.5, "otro_mundo" parsed as world, 267, 0)
		send title "&eAldea" with subtitle "&bEscoge tu aldea tocando el cristal" to player
		message "&5Sistema &8► &dPor favor escoge una aldea tocando el cristal correspondiente a ella."
		set {Creando PJ::%player%} to 4

on right click:
	if clicked block is set:
		if {Creando PJ::%player%} is 4:
			if clicked block is red stained glass or red stained glass pane:
				escogerAldea(player, "Konohagakure")
			else if clicked block is lime stained glass or lime stained glass pane:
				escogerAldea(player, "Sunagakure")
			else if clicked block is blue stained glass or blue stained glass pane:
				escogerAldea(player, "Kirigakure")
			else if clicked block is brown stained glass or brown stained glass pane:
				escogerAldea(player, "Iwagakure")
			else if clicked block is yellow stained glass or yellow stained glass pane:
				escogerAldea(player, "Kumogakure")
		else if {Creando PJ::%player%} is 5:
			if clicked block is end portal frame block:
				if {Eleccion de Clan::%player%} is not set:
					if {Escogiendo Clan::%player%} is true:
						stop
					set {Escogiendo Clan::%player%} to true
					set {_p} to player
					set {_inv} to Bukkit.createInventory(null, 27, "&lEscogiendo Clan")
					{_p}.openInventory({_inv})
					set {_c} to -1
					loop 13 times:
						add 1 to {_c}
						set slot {_c} of player's current inventory to black stained glass pane named "&0&l..."
					set {_c} to 13
					loop 13 times:
						add 1 to {_c}
						set slot {_c} of player's current inventory to black stained glass pane named "&0&l..."
					set slot 13 of player's current inventory to barrier named "&cNinguno"
					escogiendoClan(player, uncolored {Aldea::%player%})
				else:
					set {_p} to player
					set {_inv} to Bukkit.createInventory(null, 9, "&lUsando Eleccion de Clan")
					{_p}.openInventory({_inv})
					if {Aldea::%player%} contains "Konoha":
						set {_clanes::*} to "&4Uchiha", "&bHyuga" and "&2Senju"
					else if {Aldea::%player%} contains "Suna":
						set {_clanes::*} to "&aShirogane", "&eKazekage" and "&bRyuuyi"
					else if {Aldea::%player%} contains "Iwa":
						set {_clanes::*} to "&7Kinzoku", "&6Kamizuru" and "&eBakuton"
					else if {Aldea::%player%} contains "Kumo":
						set {_clanes::*} to "&9Yotsuki", "&8Raisoku" and "&cChinoike"
					else if {Aldea::%player%} contains "Kiri":
						set {_clanes::*} to "&3Hozuki", "&9Hoshigaki" and "&bYuki"
					set slot 1 of player's current inventory to yellow stained glass pane named "%{_clanes::1}%"
					set slot 2 of player's current inventory to yellow stained glass pane named "%{_clanes::2}%"
					set slot 3 of player's current inventory to yellow stained glass pane named "%{_clanes::3}%"
					set slot 5 of player's current inventory to red stained glass pane named "&cUzumaki"
					set slot 6 of player's current inventory to red stained glass pane named "&fKaguya"

on inventory click:
	if inventory name of current inventory of player = "&lEscogiendo Clan":
		cancel event
	else if inventory name of current inventory of player = "&lUsando Eleccion de Clan":
		cancel event
		if event-slot is 1:
			if {Aldea::%player%} contains "Konoha":
				close player's inventory
				make console execute command "lp user %player% parent set g_uchiha"
				set {Clan::%player%} to "&4Uchiha"
				delete {Eleccion de Clan::%player%} and {Escogiendo Clan::%{_user}%}
				set {Creando PJ::%player%} to 6
				wait 2 seconds
				close player's inventory
				send title {Clan::%player%} with subtitle "&bAhora perteneces al clan %{Clan::%player%}%" to player
				wait 2 seconds
				teleport player to location(-151.5, 5, -201.5, "otro_mundo" parsed as world, -270, 0)
				message "&5Sistema &8► &dÚltimo paso... Por favor enlaza tu cuenta de Discord con Minecraft utilizando el comando /enlazar"
				send title "&9Discord" with subtitle "&bPor favor enlaza tu cuenta de Discord con MC con /enlazar" to player
			else if {Aldea::%player%} contains "Suna":
				close player's inventory
				make console execute command "lp user %player% parent set g_shirogane"
				set {Clan::%player%} to "&aShirogane"
				delete {Eleccion de Clan::%player%} and {Escogiendo Clan::%{_user}%}
				set {Creando PJ::%player%} to 6
				wait 2 seconds
				close player's inventory
				send title {Clan::%player%} with subtitle "&bAhora perteneces al clan %{Clan::%player%}%" to player
				wait 2 seconds
				teleport player to location(-151.5, 5, -201.5, "otro_mundo" parsed as world, -270, 0)
				message "&5Sistema &8► &dÚltimo paso... Por favor enlaza tu cuenta de Discord con Minecraft utilizando el comando /enlazar"
				send title "&9Discord" with subtitle "&bPor favor enlaza tu cuenta de Discord con MC con /enlazar" to player
			else if {Aldea::%player%} contains "Iwa":
				close player's inventory
				make console execute command "lp user %player% parent set g_kinzoku"
				set {Clan::%player%} to "&7Kinzoku"
				delete {Eleccion de Clan::%player%} and {Escogiendo Clan::%{_user}%}
				set {Creando PJ::%player%} to 6
				wait 2 seconds
				close player's inventory
				send title {Clan::%player%} with subtitle "&bAhora perteneces al clan %{Clan::%player%}%" to player
				wait 2 seconds
				teleport player to location(-151.5, 5, -201.5, "otro_mundo" parsed as world, -270, 0)
				message "&5Sistema &8► &dÚltimo paso... Por favor enlaza tu cuenta de Discord con Minecraft utilizando el comando /enlazar"
				send title "&9Discord" with subtitle "&bPor favor enlaza tu cuenta de Discord con MC con /enlazar" to player
			else if {Aldea::%player%} contains "Kumo":
				close player's inventory
				make console execute command "lp user %player% parent set g_yotsuki"
				set {Clan::%player%} to "&9Yotsuki"
				delete {Eleccion de Clan::%player%} and {Escogiendo Clan::%{_user}%}
				set {Creando PJ::%player%} to 5
				wait 2 seconds
				close {_user}'s inventory
				send title {Clan::%player%} with subtitle "&bAhora perteneces al clan %{Clan::%player%}%" to player
				wait 2 seconds
				teleport player to {Creador Personaje Discord}
				send title "&9Discord" with subtitle "&bPor favor enlaza tu cuenta de Discord con MC con /enlazar" to player
			else if {Aldea::%player%} contains "Kiri":
				close player's inventory
				make console execute command "lp user %player% parent set g_hozuki"
				set {Clan::%player%} to "&3Hozuki"
				delete {Eleccion de Clan::%player%} and {Escogiendo Clan::%{_user}%}
				set {Creando PJ::%player%} to 6
				wait 2 seconds
				close player's inventory
				send title {Clan::%player%} with subtitle "&bAhora perteneces al clan %{Clan::%player%}%" to player
				wait 2 seconds
				teleport player to location(-151.5, 5, -201.5, "otro_mundo" parsed as world, -270, 0)
				message "&5Sistema &8► &dÚltimo paso... Por favor enlaza tu cuenta de Discord con Minecraft utilizando el comando /enlazar"
				send title "&9Discord" with subtitle "&bPor favor enlaza tu cuenta de Discord con MC con /enlazar" to player
		else if event-slot is 2:
			if {Aldea::%player%} contains "Konoha":
				close player's inventory
				make console execute command "lp user %player% parent set g_hyuga"
				set {Clan::%player%} to "&bHyuga"
